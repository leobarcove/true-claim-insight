# True Claim Insight - Adjuster Portal Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_URL=http://localhost:3000/api/v1

# Application Settings
VITE_APP_NAME="True Claim Insight"
VITE_APP_VERSION=0.1.0

# Feature Flags
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK_DATA=true

# Video Service (Tencent TRTC)
# These will be provided by the video-service API
VITE_TRTC_SDK_APP_ID=

# Analytics (Optional)
VITE_ANALYTICS_ID=
